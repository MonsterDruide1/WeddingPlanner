<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.tenolo.weddingplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_quadratisch"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".Startseite">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TischordnungSelector"/>
        <activity
            android:name=".ShowTischordnungAsList"/>
        <activity
            android:name=".TischordnungNames"/>
        <activity
            android:name=".Gaesteliste"/>
        <activity
            android:name=".AllgemeineSeite"/>
        <activity
            android:name=".Einstellungen"/>




        <receiver android:name=".DeviceBootReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
        <receiver android:name=".NotificationZeitplan" />
    </application>

</manifest>